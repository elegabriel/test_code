/* The isBadVersion API is defined in the parent class VersionControl.
      bool IsBadVersion(int version); */

public class Solution : VersionControl {
    public int FirstBadVersion(int n) {
            if(!isBadVersion(n))
	                return 0;

        while true
	        {
		            if(isBadVersion(n))
			                {
					                if(n==1)
							                    return 1;
									                    if(isBadVersion(n-1))
											                        n = n/2;
														                else
																                    return n;
																		                }
																				            else
																					                {
																							                if(isBadVersion(n+1))
																									                    return n+1;
																											                    else
																													                        n = n*2/3;
																																            }
																																	            }
																																		        }
																																			}/* The isBadVersion API is defined in the parent class VersionControl.
      bool IsBadVersion(int version); */

public class Solution : VersionControl {
    public int FirstBadVersion(int n) {
    	   
    }
}